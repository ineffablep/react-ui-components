    {
      "path": "/register",
      "text": "Register",
      "json": [
        {
          "component": "Form",
          "schema": {
            "children": [
              {
                "component": "BaseInput",
                "props": {
                  "labelText": "User Name",
                  "placeholder": "Phone number or Email address",
                  "required": "true",
                  "id": "userName",
                  "validateOn": "onChange",
                  "validateRules": [
                    {
                      "rule": "required",
                      "message": "Phone number or Email address is required"
                    }
                  ]
                }
              },
              {
                "component": "BaseInput",
                "props": {
                  "labelText": "Password",
                  "placeholder": "Password",
                  "required": "true",
                  "id": "password",
                  "type": "password",
                  "validateOn": "onChange",
                  "validateRules": [
                    {
                      "rule": "required",
                      "message": "Password is required"
                    }
                  ]
                }
              },
              {
                "component": "BaseInput",
                "props": {
                  "labelText": "Confirm Password",
                  "placeholder": "Confirm Password",
                  "required": "true",
                  "id": "confirmPassword",
                  "type": "password",
                  "validateOn": "onChange",
                  "validateRules": [
                    {
                      "rule": "required",
                      "message": "Confirm Password is required"
                    },
                    {
                      "rule": "equal",
                      "expression": "[confirmPassword,password]",
                      "message": "Password and Confirm Password do not match"
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
